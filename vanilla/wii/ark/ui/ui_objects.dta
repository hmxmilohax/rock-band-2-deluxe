      (loading_panel
         (change_proxies
            {if
               {exists game}
               {foreach_int
                  $i
                  0
                  {user_mgr get_max_users}
                  {do
                     ($user
                        {user_mgr get_user_from_slot $i})
                     ($track
                        {if_else
                           $user
                           {$user get_track_sym}
                           none})
                     ($object
                        {sprintf
                           "load_icon_%i"
                           $i})
                     {cond
                        ({== $track guitar}
                           {$object
                              override_proxy
                              "load_icon_guitar.milo"})
                        ({== $track bass}
                           {$object
                              override_proxy
                              "load_icon_bass.milo"})
                        ({== $track drum}
                           {$object
                              override_proxy
                              "load_icon_drum.milo"})
                        ({== $track vocals}
                           {$object
                              override_proxy
                              "load_icon_vox.milo"})
                        ({== $track none}
                           {$object
                              override_proxy
                              ""})
                        (TRUE
                           {notify
                              "Unknown track type: '"
                              $track
                              "' for player #"
                              $i
                              "!\n"})}}}}))))
