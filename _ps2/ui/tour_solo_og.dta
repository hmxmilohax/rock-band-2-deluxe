{new
   UIPanel
   tour_solo_gigcomplete_panel
   (file
      "tour_solo_gigcomplete.milo")
   (focus continue.btn)
   (enter
      {do
         ($tp
            {tour performer})
         ($performer
            {beatmatch active_player 0})
         ($event
            {tour event})
         {stars.lbl
            set_localized
            {$performer star_rating}}
         {score.lbl
            set_localized
            {fmt_score
               {$performer score}}}
         {do
            ($gender
               {$tp get_gender})
            ($category
               {$event outfit_category_unlock $gender})
            ($outfit
               {$event outfit_unlock $gender})
            {if_else
               {&&
                  {!= $category ''}
                  {!
                     {$tp has_outfit $category $outfit}}}
               {do
                  {outfit.lbl set_showing TRUE}
                  {outfit.lbl
                     set_localized
                     {sprintf
                        {localize tour_solo_outfit}
                        {localize_token $outfit}}}}
               {outfit.lbl set_showing FALSE}}}
         {do
            ($instrument
               {$tp get_instrument})
            ($outfit
               {$event instrument_unlock $instrument})
            ($category
               {if_else
                  {== $instrument vocals}
                  mic
                  $instrument})
            {if_else
               {&&
                  {!= $outfit ''}
                  {!
                     {$tp has_outfit $category $outfit}}}
               {do
                  {instrument.lbl set_showing TRUE}
                  {instrument.lbl
                     set_localized
                     {sprintf
                        {localize tour_solo_instrument}
                        {localize_token $outfit}}}}
               {instrument.lbl set_showing FALSE}}}
         {do
            ($new_town
               {$tp new_town})
            {newtown.lbl
               set_showing
               {!= $new_town ''}}
            {unless
               {== $new_town ''}
               {newtown.lbl
                  set_localized
                  {sprintf
                     {localize tour_endgame_newtown}
                     {localize $new_town}}}}}
         {do
            {newgigs.lbl set_showing $num_new_songs}}
         #ifdef HX_XBOX
         {money.lbl
            set_localized
            {sprintf
               {localize tour_solo_reward}
               {fmt_money
                  {$tp
                     reward
                     {$performer num_stars}}}}}
         #endif
         {log_tour
            "EVENT   "
            {sprintf
               "WON $%d"
               {{tour performer}
                  reward
                  {$performer num_stars}}}}
         {log_tour
            "EVENT   "
            {sprintf
               "WON *x%d"
               {$performer num_stars}}}
         {if
            {$tp is_tour_complete}
            {achievements
               submit
               {game get leader_num}
               {symbol
                  {sprintf
                     "ach_solo_%s_%s"
                     {$tp get_instrument}
                     {{game
                           get_player_config
                           {game get leader_num}}
                        get_difficulty_sym}}}}}
         {foreach
            $venue
            (big_club theater arena)
            {if
               {$tp is_venue_type_unlocked $venue}
               {achievements
                  submit
                  {game get leader_num}
                  {symbol
                     {sprintf
                        "ach_solo_venue_%s"
                        $venue}}}}}})
   (SELECT_START_MSG
      #ifdef HX_XBOX
      {ui goto_screen endgame_autosave_start_screen}
      #endif
      kDataUnhandled)}
