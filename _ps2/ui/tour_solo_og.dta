         #ifndef HX_XBOX
         {gender.grp set_showing FALSE}
         {charcreator_panel check_clear_player}
         #endif
         {gamertag.lbl
            set_localized
            {$pcfg get player_name}}
         {instrument.lbl
            set_localized
            {get_font_char_from_controller_type
               {$pcfg get_controller_type}}}
         {set
            [char_provider]
            {get_char_provider $player_num}}
         #ifndef HX_XBOX
         {[char_provider]
            set
            is_multiplayer
            FALSE}
         #endif
         {[char_provider]
            set_instrument_filter
            {$pcfg get_controller_sym}}
         {char.lst
            set_provider
            [char_provider]}
         {do
            ($char_pos
               {[char_provider]
                  get_pos_from_char
                  {$pcfg get_char}})
            {unless
               {== $char_pos -1}
               {char.lst set_selected $char_pos}}}}
      #ifdef HX_XBOX
      {$this set_gender FALSE}
      {$this update}
      #endif)
   (unload
      {practice_space_panel set_showing_callback ''}
      {charcreator_panel free_all})
   (update
      {do
         ($pos
            {char.lst selected_pos})
         ($player_num
            {game get leader_num})
         ($pcfg
            {game get_player_config $player_num})
         ($char
            {if_else
               {[char_provider]
                  is_create_new
                  $pos}
               0
               #ifndef HX_XBOX
               {[char_provider]
                  get_tour_char
                  $pos}
               #endif})
         {$pcfg set_char $char}})
   #ifdef HX_XBOX
   (SCROLL_MSG
      {$this update})
   #endif
   #ifdef HX_XBOX
   (FOCUS_MSG
      {if_else
         {'||'
            {== male.btn $new_focus}
            {== female.btn $new_focus}}
         {charcreator_panel
            request
            {game get leader_num}
            {basename
               {$new_focus name}}}
         {charcreator_panel
            free
            {game get leader_num}}})
   #endif
   (SELECT_MSG
      {if_else
         {== $component char.lst}
         {if_else
            {[char_provider]
               is_create_new
               {$component selected_pos}}
            {if_else
               {profile_mgr at_char_limit}
               {ui push_screen too_many_chars_screen}
               {do
                  {charcreator_panel
                     request
                     {game get leader_num}
                     "male"}
                  {charcreator_panel
                     set
                     char_player_num
                     {game get leader_num}}
                  {do
                     ($player_num
                        {game get leader_num})
                     ($pcfg
                        {game get_player_config $player_num})
                     {$pcfg set_char_from_charcreator charcreator_panel $player_num}
                     {tour
                        set_performer
                        {$pcfg get_tour_performer}}}
                  {ui goto_screen tour_band_create_screen}}}
            {do
               #ifndef HX_XBOX
               ($player_num
                  {game get leader_num})
               ($pcfg
                  {game get_player_config $player_num})
               {$this update}
               {tour
                  set_performer
                  {$pcfg get_tour_performer}}
               {{tour performer}
                  populate_seen_towns}
               #endif
               {ui goto_screen tour_solo_diff_screen}}}
         {do
            {charcreator_panel
               set
               char_player_num
               {game get leader_num}}
            {ui goto_screen charcreator_maker_screen}}})
   #ifdef HX_XBOX
   (set_gender
      ($enable)
      {char.grp
         set_showing
         {! $enable}}
      {gender.grp set_showing $enable}
      {$this
         set_focus
         {if_else $enable male.btn char.lst}})
   #endif
   (show_char
      ($player_num)
      {'||'
         {!=
            {$this focus_name}
            char.lst}
         {!
            {[char_provider]
               is_create_new
               {char.lst selected_pos}}}})}
