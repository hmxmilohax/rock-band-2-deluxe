               {game get_player_num_from_slot_num $slot})
            ($active
               {!= $player_num -1})
            {$name set_showing $active}
            #ifdef HX_XBOX
            {$money set_showing $active}
            #endif
            {$part set_showing $active}
            {$leader
               set_showing
               {==
                  $player_num
                  {game get leader_num}}}
