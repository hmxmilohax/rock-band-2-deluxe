               $active
               {do
                  ($char
                     {{game get_player_config $player_num}
                        get_char})
                  {$name
                     set_localized
                     {$char get_char_name}}
                  {$part
                     set_localized
                     {cnv_instrumenttoicon
                        {$char get_instrument}}}
                  #ifdef HX_XBOX
                  {$money
                     set_localized
                     {fmt_money
