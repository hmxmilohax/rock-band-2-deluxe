{new
   UIPanel
   tour_world_status
   (file
      "tour_world/tour_world_status.milo")
   (band_name
      "")
   (enter
      {do
         ($char
            {tour performer})
         {set
            [band_name]
            {$char get_char_name}}
         {bandname.lbl
            set_localized
            [band_name]}})
   (refresh
      {do
         ($char
            {tour performer})
         {if
            {'||'
               {==
                  [band_name]
                  ""}
               {!=
                  [band_name]
                  {$char get_char_name}}}
            {set
               [band_name]
               {$char get_char_name}}
            {bandname.lbl
               set_localized
               [band_name]}}
         {fame.lbl
            set_localized
            {sprintf
               "%d"
               {$char world_fame}}}
         {buzz.lbl
            set_localized
            {sprintf
               "%d"
               {$char fans}}}
         {if_else
            {!=
               {$char next_item}
               ''}
            {do
               ($body
                  {localize
                     {$this carrot_item_token status}})
               ($suffix
                  {sprintf
                     "    %s"
                     {$this carrot_desc_str status}})
               ($x 0)
               ($y 0)
               ($z 0)
               {carrot_hint.lbl
                  set_localized
                  {sprintf
                     "%s%s"
                     $body
                     $suffix}}
               {carrot_hint.lbl
                  get_char_pos
                  {strlen $body}
                  $x
                  $y
                  $z}
               {carrot_star.lbl
                  set_local_pos
                  $x
                  {'+' $y 4}
                  $z}
               {if_else
                  {!=
                     kTourLocked
                     {$char
                        world_event_status
                        {$char next_item_event}}}
                  {do
                     {carrot_hint.lbl set_state kNormal}
                     {carrot_star.lbl set showing FALSE}}
                  {do
                     {carrot_hint.lbl set_state kDisabled}
                     {carrot_star.lbl set showing TRUE}}}}
            {do
               {carrot_hint.lbl set showing FALSE}
               {carrot_star.lbl set showing FALSE}}}})
   (carrot_item_token
      ($prefix)
      {do
         ($tp
            {tour performer})
         ($carrot_event
            {$tp next_item_event})
         ($item
            {$carrot_event get award_item})
         #ifndef _SHIP
         {unless
            $carrot_event
            {fail
               "no $carrot_event!"}}
         #endif
         {if_else
            {!=
               kTourLocked
               {$tp world_event_status $carrot_event}}
            {sprintf
               "%s%s_carrot_canwin"
               $prefix
               $item}
            {sprintf
               "%s%s_carrot_needmore"
               $prefix
               $item}}})
   (carrot_desc_str
      ($prefix)
      {do
         ($tp
            {tour performer})
         ($carrot_event
            {$tp next_item_event})
         ($item
            {$carrot_event get award_item})
         #ifndef _SHIP
         {unless
            $carrot_event
            {fail
               "no $carrot_event!"}}
         #endif
         {if_else
            {!=
               kTourLocked
               {$tp world_event_status $carrot_event}}
            ""
            {do
               ($world_fame
                  {$tp world_fame})
               ($required_world_fame
                  {$carrot_event get world_fame_required})
               ($town
                  {$tp find_event_town $carrot_event})
               ($town_fame
                  {$tp town_fame $town})
               ($required_town_fame
                  {$carrot_event get town_fame_required})
               ($fans
                  {$tp fans})
               ($required_fans
                  {$tp
                     buzz_to_fans
                     {$carrot_event get buzz_required}})
               {cond
                  ({> $required_world_fame $world_fame}
                     {sprintf
                        "%d %s"
                        {- $required_world_fame $world_fame}
                        {localize
                           {sprintf
                              "%stour_band_need_worldfame"
                              $prefix}}})
                  (TRUE
                     {notify
                        "Could not determine carrot desc for event "
                        {$carrot_event display_name}
                        ", item "
                        $item}
                     "")}}}})}
{new
   UIPanel
   tour_band_char_panel
   (file
      "tour/tour_band_sel.milo")
   (reset_focus TRUE)
   (player_num -1)
   (char_provider
      {object
         ""})
   (char_list
      {tour_band_sel_render find band.lst})
   (enter
      {helpbar set_limit_to_session FALSE}
      {helpbar set_limit_to_player_num -1}
      {$this
         set_focus
         {tour_band_sel_render find band.lst}}
      {meta music_stop}
      {do
         ($player_num
            {game get leader_num})
         ($pcfg
            {game get_player_config $player_num})
         {$pcfg set is_multiplayer TRUE}
         {$pcfg set_char 0}
         {set
            [char_provider]
            {get_char_provider $player_num}}
         {[char_provider]
            set
            is_multiplayer
            TRUE}
         {[char_provider]
            set_instrument_filter
            {$pcfg get_controller_sym}}
         {{tour_band_sel_render find band.lst}
            set_provider
            [char_provider]}
         {do
            ($char_pos
               {[char_provider]
                  get_pos_from_char
                  {$pcfg get_char}})
            {unless
               {== $char_pos -1}
               {{tour_band_sel_render find band.lst}
                  set_selected
                  $char_pos}}}})
   (unload
      {charcreator_panel free_all})
   (update
      {do
         ($pos
            {{tour_band_sel_render find band.lst}
               selected_pos})
         ($player_num
            {game get leader_num})
         ($pcfg
            {game get_player_config $player_num})
         ($char
            {if_else
               {[char_provider]
                  is_create_new
                  $pos}
               0
               {[char_provider]
                  get_tour_char
                  $pos}})
         {$pcfg set_char $char}})
   (SELECT_MSG
      {if_else
         {==
            $component
            {tour_band_sel_render find band.lst}}
         {if_else
            {[char_provider]
               is_create_new
               {$component selected_pos}}
            {if_else
               {profile_mgr at_char_limit}
               {ui push_screen too_many_chars_screen}
               {do
                  {game set leader_num $player_num}
                  {charcreator_panel
                     request
                     {game get leader_num}
                     "male"}
                  {charcreator_panel
                     set
                     char_player_num
                     {game get leader_num}}
                  {do
                     ($player_num
                        {game get leader_num})
                     ($pcfg
                        {game get_player_config $player_num})
                     {$pcfg set_char_from_charcreator charcreator_panel $player_num}
                     {tour
                        set_performer
                        {$pcfg get_tour_performer}}}
                  {ui goto_screen matchmaking_band_screen}}}
            {do
               ($player_num
                  {game get leader_num})
               ($pcfg
                  {game get_player_config $player_num})
               {$this update}
               {tour
                  set_performer
                  {$pcfg get_tour_performer}}
               {{tour performer}
                  populate_seen_towns}
               {session clear}
               {game set leader_num -1}
               {ui goto_screen matchmaking_band_screen}}}
         {do
            {charcreator_panel
               set
               char_player_num
               {game get leader_num}}
            {ui goto_screen charcreator_maker_screen}}})
   (show_char
      ($player_num)
      {'||'
         {!=
            {$this focus_name}
            {tour_band_sel_render find band.lst}}
         {!
            {[char_provider]
               is_create_new
               {{tour_band_sel_render find band.lst}
                  selected_pos}}}})}
