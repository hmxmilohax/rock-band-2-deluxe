         ($perf
            {tour performer})
         {stats.grp
            set_showing
            {!=
               {$perf world_fame}
               0}}
         {fame.lbl
            set_localized
            {fmt_score
               {$perf world_fame}}}
         {buzz.lbl
            set_localized
            {$perf fans_str}}}
      {foreach_int
         $slot
         0
         {player_names.set
            size
            (objects)}
         {do
            ($name
               {player_names.set
                  get
                  (objects $slot)})
            ($part
               {player_parts.set
                  get
                  (objects $slot)})
            #ifdef HX_XBOX
            ($money
               {player_money.set
