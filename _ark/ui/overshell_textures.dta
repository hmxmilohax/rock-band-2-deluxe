               (bass
                  #ifdef HX_WII
                  {anchor_corner02.tex set_bitmap {sprint "track_og/spotlight/" $spotlightdrum ".png"}}
                  #else
                  {anchor_corner02.tex set_bitmap {sprint "track_new/spotlight/" $spotlightbass ".png"}}
                  #endif
                  {title.lbl set_localized {localize os_spotlightbass}}
               )
               (drum
