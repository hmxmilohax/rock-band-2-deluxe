                  {anchor_corner02.tex set_bitmap {sprint "track_og/spotlight/" $spotlightdrum ".png"}}
                  #else
                  {anchor_corner02.tex set_bitmap {sprint "track_new/spotlight/" $spotlightdrum ".png"}}
