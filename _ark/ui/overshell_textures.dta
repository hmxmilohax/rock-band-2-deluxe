         (voxarrow
            {sel_section.lst set_data (#include ../track/voxarrow/voxarrow.dta)}
            {title.lbl set_localized {localize os_voxarrow}}
            {anchor_corner02.tex set_bitmap {sprint "track/voxarrow/pitch_arrow_split_" $voxarrow ".png"}}
