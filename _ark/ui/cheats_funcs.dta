#define CHEAT_DISPLAY_S
(5)
{new
   Object
   cheat_display
   (show
      ($msg)
      {rnd show_overlay cheats TRUE}
      {rnd
         reflect
         cheats
         {print
            $msg
            "\n"}}
      {'++'
         [refs]}
      {script_task
         kTaskUISeconds
         (delay CHEAT_DISPLAY_S)
         (script
            {--
               [refs]}
            {if
               {==
                  [refs]
                  0}
               {rnd show_overlay cheats FALSE}})})
   (show_bool
      ($label $val)
      {$this
         show
         {sprint
            $label
            ": "
            {if_else
               $val
               "ON"
               "OFF"}}})
   (refs 0)}
