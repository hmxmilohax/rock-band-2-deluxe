      thread_task
      kTaskBeats
      (preserve $ms $auto0 $auto1)
      {beatmatch set_time_paused TRUE}
      {set
         $ms
         {taskmgr ms}}
      {set
         $auto0
         {{beatmatch active_player 0}
            auto_play}}
      {if
         {game multiplayer}
         {set
            $auto1
            {{beatmatch active_player 1}
               auto_play}}}
      {game
         load_song
         {game get_song}}
      {$task
         wait
         {!
            {game is_loaded}}}
      {cheat_display
         show
         {sprint
            "Reloaded song: "
            {game get_song}}}
      {game set_skip_intro TRUE}
      {game set_start_paused TRUE}
      {game start_song_now}
      {{get_track_panel}
         set_track_out}
      {beatmatch jump $ms}
      {game set_skip_intro FALSE}
      {game set_start_paused TRUE}
      {{beatmatch active_player 0}
         set_auto_play
         $auto0}
      {if
         {game multiplayer}
         {{beatmatch active_player 1}
            set_auto_play
            $auto1}}}}
