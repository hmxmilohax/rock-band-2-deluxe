   UIPanel
   hth_split_seldiff_panel
   #ifndef HX_XBOX
   (file
      "seldiff_hth_og.milo")
   #else
   (file
      "seldiff_hth.milo")
   #endif
   LIMIT_TO_SESSION
   (load
      {qp_hth_seltrack_panel load})
