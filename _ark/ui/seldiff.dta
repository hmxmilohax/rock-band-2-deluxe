      {session set_all_local_ready_status FALSE}))
{new
   UIPanel
   seldiff_panel
   #ifdef HX_XBOX
   (file
      "seldiff.milo")
   #else
   (file
      "seldiff_og.milo")
   #endif
   SELDIFF_PANEL_HANDLERS
   LIMIT_TO_SESSION
   (enter
      {do
         ($perf
            {tour performer})
         ($num_songs
            {meta_performer num_songs})
         {song.lbl
            set_localized
            {if_else
               $perf
               {if_else
                  {gamemode is_tour_challenge}
                  {{$perf event_challenge}
                     display_name}
                  {tour event_display_name}}
               {if_else
                  {> $num_songs 1}
                  {localize
                     "qp_setlist"}
                  {song_mgr
                     song_name
                     {meta_performer song}}}}}
         {by_line.lbl
            set_localized
            {if_else
               $perf
               {tour event_description}
               {if_else
                  {> $num_songs 1}
                  {sprintf
                     {localize
                        "qp_setlist_byline"}
                     $num_songs}
                  {song_mgr
