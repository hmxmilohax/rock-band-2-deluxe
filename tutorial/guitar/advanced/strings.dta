               0}
            {set
               [num_gems_hit]
               0}}})
   (passed
      {>=
         [num_runs_hit]
         STRING_RUNS_TO_HIT})
   (failed
      {!
         {$this passed}})
   (lesson_running
      {&&
         {<
            [num_runs_hit]
            STRING_RUNS_TO_HIT}
         {<
            {{beatmatch active_player 0}
               percent_complete}
            END_OF_SONG_PERCENT}})
   (num_runs_hit 0)
   (num_gems_hit 0)
   (num_gems 0)
