                     TRUE}
                  {play_vo tute_guitar3_trillhinthold}}
               {set
                  [feedback_played]
                  FALSE}}}})
   (miss
      {set
         [num_gems_hit]
         0})
   (pass
      {$this miss}
      {$this update_num_gems})
   (update_num_gems
      {set
         [num_gems]
         {'+'
            [num_gems]
            1}}
      {if
