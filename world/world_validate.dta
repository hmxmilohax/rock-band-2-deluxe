                  {$cam get near_plane}}}
            {notify
               {path_name $cam}
               ": "
               {$cam get far_plane}
               "/"
               {$cam get near_plane}
               " far/near plane ratio is 1000 or greater."}}
         {if
            {&&
               $video_venue
               {find_elem
                  (INTRO_QUICK INTRO_MULTI INTRO)
                  {$cam get category}}}
            {notify
               {path_name $cam}
               " should not exist, video venues should have only INTRO_VENUE"}}
         {foreach_int
            $i
            0
            {$cam
               size
               (targets)}
            {do
               ($anim
                  {$cam
