                        {find_elem
                           (LOSE WIN WIN_PRE WIN_POST WIN_P1 WIN_P2 WIN_TIE WIN_QUICK INTRO INTRO_VENUE INTRO_QUICK INTRO_MULTI OUTRO OUTRO_MULTI WIN_FINALE WIN_PRE_FINALE)
                           {$cam get category}}}}
                  {notify
                     {path_name $cam}
                     " uses realtime_idle group but is category "
                     {$cam get category}}}}}
         {if
            {has_substr
               {$cam name}
               coop}
            {foreach_int
               $i
               0
               {$cam
