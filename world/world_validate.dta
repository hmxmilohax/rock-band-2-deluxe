                  {unless
                     {& $flags kValidAllAttitude}
                     {notify
                        {path_name $cam}
                        " is an instrument directed cut and has no valid attitude"}}}
               {if
                  {!=
                     {& $flags kValidAll}
                     kValidAll}
                  {notify
                     {path_name $cam}
                     " is not a gender/genre specific directed cut and has some valid flags not set"}}}}
         {do
            ($next
               {$cam get next_shot})
            {if
               $next
               {foreach_int
                  $i
                  0
                  {$next
                     size
                     (targets)}
                  {unless
                     {$next
                        get
                        (targets $i teleport)}
                     {foreach_int
                        $j
                        0
                        {$cam
                           size
                           (targets)}
                        {if
                           {&&
                              {==
                                 {$next
                                    get
                                    (targets $i target)}
                                 {$cam
                                    get
                                    (targets $j target)}}
                              {$cam
                                 get
                                 (targets $j return)}}
                           {notify
                              {path_name $next}
                              " target "
                              {$next
                                 get
                                 (targets $i target)}
                              " does not teleport, and is next_shot of "
                              $cam
