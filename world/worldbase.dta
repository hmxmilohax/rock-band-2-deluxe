#define WORLD_OBJECT_BASE
((excitement_level kExcitementOkay)
   (last_excitement_level -1)
   WORLD_MODE_COMMON
   (start kDataUnhandled)
   (coda_success
      {handle
         ($this bre_success)}
      {handle
         ($this
            set_lighting
            VERSECHORUS
            (win_bre))})
   (spotlight_bass_on
