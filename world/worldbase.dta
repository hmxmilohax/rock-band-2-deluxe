      {set
         [last_excitement_level]
         -1}
      {set $camera.postproc_override FALSE}
      {set $camera.postproc_blendtime 0.0}
      {set $camera.postproc_cache ''}
      {unless
         {exists game}
         {set $game.play_mode coop}
         {$this sync_mode}}
      {stagekit_reset}
      {if
         {exists game}
         {$this
            iterate_shot
            $shot
            {with
               $shot
               {mask_eq
                  [flags]
                  kBCSBOneTimePlayed}}}
         {set_camera_solo_mode
